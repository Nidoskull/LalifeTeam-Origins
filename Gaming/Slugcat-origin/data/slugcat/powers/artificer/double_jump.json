{
    "type": "origins:multiple",
    "air": {
        "type": "origins:active_self",
        "hud_render": {
            "should_render": false
        },
        "cooldown": 5,
        "key": {
            "key": "key.jump"
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [{
                    "type": "origins:change_resource",
                    "resource": "*:*_jumps",
                    "change": -1
                },
                {
                    "type": "origins:set_fall_distance",
                    "fall_distance": 0
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:moving"
                    },
                    "if_action": {
                        "type": "origins:add_velocity",
                        "y": 0.55,
                        "z": 0.35,
                        "space": "local_horizontal"
                    },
                    "else_action": {
                        "type": "origins:add_velocity",
                        "y": 0.7,
                        "space": "local_horizontal"
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:explosion ~ ~ ~ 0.3 0.3 0.3 0.0 2 normal @a"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.generic.explode player @a ~ ~ ~ 1 1.7"
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [{
                    "type": "origins:resource",
                    "resource": "*:*_jumps",
                    "comparison": "!=",
                    "compare_to": 0
                },
                {
                    "type": "origins:on_block",
                    "inverted": true
                },
                {
                    "type": "origins:climbing",
                    "inverted": true
                },
                {
                    "type": "origins:riding",
                    "inverted": true
                },
                {
                    "type": "origins:swimming",
                    "inverted": true
                },
                {
                    "type": "origins:submerged_in",
                    "fluid": "minecraft:water"
                },
                {
                    "type": "origins:gamemode",
                    "gamemode": "creative",
                    "inverted": true
                },
                {
                    "type": "origins:gamemode",
                    "gamemode": "spectator",
                    "inverted": true

                }
            ]
        }
    },
    "jumps": {
        "type": "origins:resource",
        "min": 0,
        "max": 5,
        "start_value": 5,
        "hud_render": {
            "should_render": false
        }
    },
    "reset": {
        "type": "origins:action_over_time",
        "interval": 5,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_jumps",
            "change": 5,
            "operation": "set"
        },
        "condition": {
            "type": "origins:and",
            "conditions": [{
                    "type": "origins:resource",
                    "resource": "*:*_jumps",
                    "comparison": "!=",
                    "compare_to": 5
                },
                {
                    "type": "origins:on_block"
                }
            ]
        }
    },
    "overcharge": {
        "type": "origins:damage_over_time",
        "damage": 4,
        "damage_source": {
            "name": "inFire"
        },
        "condition": {
            "type": "origins:resource",
            "compare_to": 2,
            "comparison": "<=",
            "resource": "*:*_jumps"
        },
        "interval": 20
    },
    "name": "Double Jump",
    "description": "Jump mid-air for an explosive launch! But don't jump TOO much, or you might blow yourself up by accident..."
}