{
  "type": "origins:multiple",
  "name": "Nimble.",
  "description": "You can climb up a few blocks.",
  "climbed_blocks": {
    "type": "origins:resource",
    "min": 0,
    "max": 17,
    "hud_render": {
      "should_render": true,
      "inverted": true,
      "bar_index": 0
    }
  },
  "climbing": {
    "type": "origins:climbing",
    "allow_holding": false,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:collided_horizontally"
        },
        {
          "type": "origins:resource",
          "resource": "*:*_climbed_blocks",
          "comparison": "<=",
          "compare_to": 16
        }
      ]
    }
  },
  "start": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_climbed_blocks",
      "change": 1
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:block_collision",
              "offset_x": 0.1,
              "offset_y": 0,
              "offset_z": 0.1
            },
            {
              "type": "origins:block_collision",
              "offset_x": -0.1,
              "offset_y": 0,
              "offset_z": -0.1
            }
          ]
        },
        {
          "type": "origins:fall_distance",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:on_block",
          "inverted": true
        }
      ]
    }
  },
  "end": {
    "type": "origins:action_over_time",
    "interval": 4,
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_climbed_blocks",
      "change": -1
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_climbed_blocks",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:on_block"
        }
      ]
    }
  }
}